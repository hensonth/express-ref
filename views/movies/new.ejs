<h1>NEW MOVIE FORM</h1>

<form method="post" action="/movies">
  <div class="form-group">
    <label for="name">NEWMOVIE[NAME]</label>
    <input type="text" class="form-control" id="name" name="newmovie[name]" placeholder="Movie Name">
  </div>
  <div class="form-group">
    <label for="year">NEWMOVIE[YEAR]</label>
    <input type="text" class="form-control" id="year" name="newmovie[year]" placeholder="Movie Year">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>

