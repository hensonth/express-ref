<%# include relative path to the partials file %>
<% include ../partials/header.ejs  %>

<h1>NEW USER FORM</h1>

<form method="post" action="/movies">
  <div class="form-group">
    <label for="name">Username</label>
    <input type="text" class="form-control" id="name" name="newuser[name]" placeholder="Put your username">
  </div>
  <div class="form-group">
    <label for="year">Password</label>
    <input type="password" class="form-control" id="year" name="newuser[password]" placeholder="Key in your password">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>

